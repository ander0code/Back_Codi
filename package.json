{
  "name": "codi-backend",
  "version": "1.0.0",
  "description": "Backend para la aplicaci√≥n CODI - Recibos Verdes",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prebuild": "prisma generate",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "prisma generate && tsx watch source/index.ts",
    "prisma:studio": "prisma studio",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "jest",
    "lint": "eslint --ext .ts source/",
    "clean": "rimraf dist/ && find . -name '*.js' -not -path './node_modules/*' -not -path './dist/*' -delete",
    "clean:cache": "rimraf node_modules/.prisma *.tsbuildinfo .pnpm-store && pnpm prisma:generate"
  },
  "keywords": [
    "codi",
    "recibos",
    "sostenibilidad",
    "impacto-ambiental",
    "express",
    "typescript"
  ],
  "author": "CODI Team",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "@google-cloud/vision": "^4.0.2",
    "@prisma/client": "^5.22.0",
    "mongoose": "^8.1.0",
    "mysql2": "^3.7.0",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7",
    "tesseract.js": "^5.0.3"
  }
}
